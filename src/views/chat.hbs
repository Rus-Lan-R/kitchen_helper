<div data-users>

</div>
<div class="chat">
	<ul data-chatWr>
		{{#each allMessages}}
			<li>
				{{this.authorID.name}}
				->
				{{this.message}}
			</li>
		{{/each}}
	</ul>
</div>

<form name="message">
	<input type="text" name="message" placeholder="Type message here..." />
	<button>Send</button>
</form>

<script src="/js/chat.js"></script>
